{
  "name": "@swimlane/docspa-remark-preset",
  "version": "2.0.1",
  "description": "Remark preset for DocSPA",
  "main": "dist/main/index.js",
  "typings": "dist/main/index.d.ts",
  "module": "dist/module/index.js",
  "scripts": {
    "clean": "trash dist",
    "build": "npm run clean && npm run build:main && npm run build:module",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "prepack": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^11.11.6",
    "@types/unist": "^2.0.3",
    "@types/vfile": "^3.0.2",
    "common-tags": "^1.8.0",
    "jest": "^24.5.0",
    "nyc": "^13.3.0",
    "trash-cli": "^1.4.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.4000"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@rigor789/remark-autolink-headings": "^5.1.0",
    "md-attr-parser": "^1.2.1",
    "mdast-util-to-string": "^1.0.5",
    "mdast-util-toc": "^3.1.0",
    "parse-numeric-range": "0.0.2",
    "prismjs": "^1.15.0",
    "remark-attr": "^0.8.3",
    "remark-frontmatter": "^1.2.1",
    "remark-gemoji-to-emoji": "^1.1.0",
    "remark-html-emoji-image": "^1.0.0",
    "remark-html-katex": "^1.1.1",
    "remark-math": "^1.0.6",
    "remark-parse": "^6.0.3",
    "rehype-raw": "^4.0.0",
    "remark-rehype": "^4.0.0",
    "remark-parse-yaml": "0.0.2",
    "remark-shortcodes": "^0.2.1",
    "remark-slug": "^5.1.0",
    "remark-stringify": "^6.0.4",
    "rehype-stringify": "^5.0.0",
    "space-separated-tokens": "^1.1.2",
    "unist-util-visit": "^1.4.0",
    "vfile-reporter": "^5.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/docspa.git"
  },
  "keywords": [
    "angular",
    "documentation"
